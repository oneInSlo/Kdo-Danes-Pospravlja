frontend/js/ajax/Request.js
class Request {
  constructor(url) {
    this.method = 'GET'
    this.url = url
  }

  onSuccess(callback) {
    this.callback = callback
    return this
  }

  onError(errorHandler) {
    this.errorHandler = errorHandler;
    return this
  }

  content(type) {
    if (type == 'json') {
      this.contentType = 'application/json'
    }
    return this
  }


  method(method) {
    this.method = method
    return this
  }

  body(body) {
    this.body = body
    return this
  } 

  send() {
    fetch(url, {
      method: this.method,
      body: this.body,
      headers: {
        "Content-Type": this.contentType
      }
    }).then(res => {
      if (!res.ok) this.errorHandler.handle(res.status, res.body.message)
      else return res.json()
    }).then(resData => {
      this.callback(resData)
    }).catch((err) => {
      this.errorHandler.handle()
    })
  }

}

export default Request
----
frontend/js/components/ErrorHandler.js
class ErrorHandler {
  handle(status, message) {
    console.log(status, message)
  }
}

export default ErrorHandler
