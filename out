        perspective: 1000px;
      }

      .flip-card-inner {
        position: relative;
        height: 100%;
        text-align: center;
        transition: transform 0.6s;
        transform-style: preserve-3d;
      }

      .flip-card-front,
      .flip-card-back {
        position: absolute;
        height: 100%;
        backface-visibility: hidden;
      }

      .flip-card-front {
      }

      .flip-card-back {
        transform: rotateY(180deg);
      }

