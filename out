    <script type="module" defer>
        import OneTimeTaskCard from './js/components/OneTimeTask/OneTimeTaskCard.js'
        import OneTimeTaskModel from './js/components/OneTimeTask/OneTimeTaskModel.js'

        const view = new OneTimeTaskCard(document.getElementById('oneTimeTasksContainer'))
        const model = new OneTimeTaskModel()
        model.addListener(view)

        model.load({
            id: 'task1',
            title: 'ciscenje',
            duration: 30, 
            location: 'xyz',
            dateAdded: '2023-10-01',
            lastChange: '2023-10-05',
            responsibleUsers: ['User1', 'User2', 'User3'],
            dueDate: '2023-10-15'
        })
        view.setModel(model)
        view.render()

    </script>

</head>

