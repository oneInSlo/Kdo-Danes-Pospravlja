frontend/js/components/OneTimeTask/OneTimeTaskEdit.js
class OneTimeTaskEdit {
  constructor() {
    this.parent = document.querySelector('body')
  }

  setModel(model) {
    this.model = model
  }

  dataChanged() {
    this.render()
  }

  close() {
    this.model.removeListener(this)
    this.modalController.hide()
    this.component.remove()
  }

  show() {
    if (!this.hasOwnProperty('modalController')) {
      this.modalController = new bootstrap.Modal(this.component)
    }

    this.modalController.show()
  }

  render() {
    if (!this.hasOwnProperty('component')){
      this.component = document.createElement('div')
      this.component.classList.add('modal', 'fade')
      this.component.setAttribute('tabindex', '-1')
      this.component.setAttribute('aria-labelledby', 'urediPodrobnostiEnkratnoLabel')
      this.component.setAttribute('aria-hidden', 'true')

    }

    this.component.remove();

    this.component.innerHTML = `
            <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable">
            <div class="modal-content">
                <div class="modal-header bg-powder">
                <h1 class="modal-title fs-4" id="urediPodrobnostiEnkratnoLabel">Uredi podrobnosti</h1>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                    <form>
                        <div class="mb-3">
                          <label for="task-name" class="form-label">Naziv opravila</label>
                          <input type="text" class="form-control" name="title" value="${this.model.title}">
                        </div>
                        <div class="mb-3">
                            <label for="location" class="form-label">Prostor</label>
                            <select class="form-select" aria-label="Select space" name="location">
                                <option selected>Izberite prostor</option>
                                <option value="1">Ni relevantan</option>
                                <option value="2">Kuhinja</option>
                                <option value="3">Kopalnica</option>
                                <option value="4">Dnevna soba</option>
                                <option value="5">Garaža</option>
                                <option value="6">Zelenica</option>
                            </select>
                        </div>
                        <div class="mb-3">
                            <label for="duration" class="form-label">Trajanje opravila</label>
                            <input type="number" class="form-control" name="duration" min="1" value="${this.model.duration}">
                        </div>
                        <div class="mb-3">
                            <label for="startDate" class="form-label">Datum izvedbe opravila</label>
                            <input type="date" class="form-control" name="dueDate" value=${this.model.dueDate}>
                        </div>
                        <div class="mb-3">
                            <p class="card-text mb-0"> Odgovorne osebe:
                                <div class="input-group mt-2">
                                    <span class="input-group-text" id="basic-addon3">
                                        <lord-icon
                                            src="https://cdn.lordicon.com/unukghxb.json"
                                            trigger="hover"
                                            colors="primary:#121331,secondary:#000"
                                            style="width:20px;height:20px">
                                        </lord-icon>
                                    </span>
                                    <input type="text" class="form-control" id="basic-url" aria-describedby="basic-addon3 basic-addon4">
                                  </div>
                                <span class="d-inline">
                                    <span class="bg-powder p-2 rounded me-2 d-inline-block m-1 mt-3">
                                        Oseba 1
                                        <button type="button" class="btn btn-close btn-close-small ms-2"></button>
                                    </span>
                                    <span class="bg-powder p-2 rounded me-2 d-inline-block m-1">
                                        Oseba 2
                                        <button type="button" class="btn btn-close btn-close-small ms-2"></button>
                                    </span>
                                    <span class="bg-powder p-2 rounded me-2 d-inline-block m-1 mt-3">
                                        Oseba 1
                                        <button type="button" class="btn btn-close btn-close-small ms-2"></button>
                                    </span>
                                </span>
                            </p>                                                  
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="submit" class="button-80 button-80-powder btnSave">Shrani spremembe</button>
                        <button type="button" class="button-80 button-80-bright-pink btnClose">Zapri</button>
                    </div>
                </form>  
            </div>
            </div>

    `

    this.component.querySelector('.btnClose').addEventListener('click', () => {
      this.close();
    })

    this.component.querySelector('.btnSave').addEventListener('click', () => {
      const formData = new FormData(this.component.querySelector('form')) 
      let key, value
      for ([key, value] of formData.entries()){ 
        const update = {}
        update[key] = value
        this.model.update(update)
      }
    })

    this.parent.appendChild(this.component)
    this.show()

  }
}

export default OneTimeTaskEdit
----
frontend/js/components/OneTimeTask/OneTimeTaskModel.js
import config from '../../config.js'
import Request from '../../ajax/Request.js'
import ErrorHandler from '../ErrorHandler.js'

class OneTimeTaskModel {

  constructor() {
    this.changeListeners = []
  }

  load(data) {
    this.id = data.id
    this.title = data.title
    this.duration = data.duration
    this.location = data.location
    this.dateAdded = data.dateAdded
    this.lastChange = data.lastChange
    this.responsibleUsers = data.responsibleUsers
    this.dueDate = data.dueDate
  }

  addListener(listener) {
    this.changeListeners.push(listener)
  } 

  removeListener(listener) {
    for (let i = 0; i < this.changeListeners.length; i++) {
      if (this.changeListeners[i] == listener) {
        this.changeListeners.splice(i, 1)
      }
    }
  }

  commit() {
    new Request(config.BASE_URL + 'tasks/' + this.id)
      .onError(new ErrorHandler())
      .onSuccess((res) => {console.log('sent')})
      .method('PUT')
      .content('json')
      .body(JSON.stringify(this))
      .send()
  }

  finishTask() {
     console.log('Task finished')
  }

  update(data) {
    Object.keys(data).forEach(key => {
      if (this.hasOwnProperty(key)) {
        this[key] = data[key]
      }
    })


    this.changeListeners.forEach(listener => listener.dataChanged())
  }

}

export default OneTimeTaskModel
----
frontend/js/config.js
const config = {
  BASE_URL: 'http://localhost:8000/'
}

export default config
